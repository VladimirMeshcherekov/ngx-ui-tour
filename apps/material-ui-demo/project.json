{
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "name": "material-ui-demo",
  "projectType": "application",
  "tags": ["apps"],
  "generators": {
    "@schematics/angular:component": {
      "style": "scss"
    }
  },
  "sourceRoot": "apps/material-ui-demo/src",
  "prefix": "app",
  "targets": {
    "build": {
      "executor": "@angular-devkit/build-angular:application",
      "options": {
        "outputPath": "dist/material-ui-demo",
        "index": "apps/material-ui-demo/src/index.html",
        "browser": "apps/material-ui-demo/src/main.ts",
        "polyfills": [
          "zone.js",
          "@angular/localize/init"
        ],
        "tsConfig": "apps/material-ui-demo/tsconfig.app.json",
        "inlineStyleLanguage": "scss",
        "assets": [
          "apps/material-ui-demo/src/favicon.ico",
          "apps/material-ui-demo/src/assets"
        ],
        "styles": [
          "apps/material-ui-demo/src/styles.scss"
        ],
        "scripts": []
      },
      "configurations": {
        "production": {
          "budgets": [
            {
              "type": "initial",
              "maximumWarning": "500kb",
              "maximumError": "1mb"
            },
            {
              "type": "anyComponentStyle",
              "maximumWarning": "2kb",
              "maximumError": "4kb"
            }
          ],
          "outputHashing": "all"
        },
        "development": {
          "optimization": false,
          "extractLicenses": false,
          "sourceMap": true
        }
      },
      "defaultConfiguration": "production"
    },
    "serve": {
      "executor": "@angular-devkit/build-angular:dev-server",
      "configurations": {
        "production": {
          "buildTarget": "material-ui-demo:build:production"
        },
        "development": {
          "buildTarget": "material-ui-demo:build:development"
        }
      },
      "defaultConfiguration": "development"
    },
    "lint": {
      "executor": "@angular-eslint/builder:lint",
      "options": {
        "lintFilePatterns": [
          "apps/material-ui-demo/**/*.ts",
          "apps/material-ui-demo/**/*.html"
        ]
      },
      "outputs": [
        "{options.outputFile}"
      ]
    }
  }
}