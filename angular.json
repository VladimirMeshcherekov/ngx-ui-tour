  "schematics": {
    "@schematics/angular:component": {
      "prefix": "app",
      "style": "scss",
      "skipTests": true,
      "changeDetection": "OnPush",
      "standalone": true
    },
    "@schematics/angular:directive": {
      "prefix": "app",
      "skipTests": true,
      "standalone": true
    },
    "@schematics/angular:class": {
      "skipTests": true
    },
    "@schematics/angular:pipe": {
      "skipTests": true,
      "standalone": true
    },
    "@schematics/angular:service": {
      "skipTests": true
    }
  },
  "cli": {
    "schematicCollections": [
      "@angular-eslint/schematics"
    ]
  }
